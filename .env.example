# Database Configuration (REQUIRED for database connection)
# Option 1: Use individual database variables
DB_USER=postgres
DB_PASSWORD=your_password
DB_HOST=localhost
DB_PORT=5432
DB_NAME=fixing_service_db
DB_DRIVER=psycopg2

# Option 2: Or use a complete DATABASE_URL
# DATABASE_URL=postgresql+psycopg2://postgres:your_password@localhost:5432/fixing_service_db

# JWT Configuration (OPTIONAL - only needed for /auth/login endpoint)
# If not provided, a default value will be used (not recommended for production)
SECRET_KEY=your-secret-key-change-this-in-production
ACCESS_TOKEN_EXPIRE_MINUTES=30
ALGORITHM=HS256
